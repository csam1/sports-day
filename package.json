{
  "name": "sports-day",
  "version": "1.0.0",
  "description": "ReactJS web app which helps a user to register for events in the Sports day.",
  "keywords": [
    "react",
    "sports day"
  ],
  "main": "src/index.tsx",
  "homepage": "https://csam1.github.io/sports-day",
  "dependencies": {
    "axios": "1.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "gh-pages": "^6.0.0",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  }
}
